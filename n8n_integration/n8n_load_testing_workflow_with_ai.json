    {
      "parameters": {
        "jsCode": "const data = $input.first().json;\nif (!data || !data.data || !data.data.result) {\n  return [{ json: { content: 'No logs found', filename: 'empty.log' } }];\n}\n\nconst logs = [];\nfor (const stream of data.data.result) {\n  for (const entry of stream.values) {\n    const timestamp = new Date(parseInt(entry[0]) / 1000000);\n    logs.push(`${timestamp.toISOString()} ${entry[1]}`);\n  }\n}\n\nconst content = logs.join('\\n');\nconst placeholder = $('Prepare Logs').item.json.placeholder;\nconst service = $('Prepare Logs').item.json.service;\n\nreturn [{\n  json: {\n    content: content,\n    filename: `${placeholder}_${service}.log`,\n    placeholder: placeholder,\n    service: service\n  }\n}];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [960, 420],
      "id": "parse-logs",
      "name": "Parse Logs"
