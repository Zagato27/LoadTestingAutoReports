        "url": "=http://ai-analyzer:5001/analyze",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"start\": {{ $('Parse Input').item.json.start_timestamp }},\n  \"end\": {{ $('Parse Input').item.json.end_timestamp }}\n}",
        "options": {
          "timeout": 300000
        }
